{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true,
        "commonjs": true,
        "jest": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "import/extensions": [
            "error",
            {
                "js": "ignorePackages",
                "json": "ignorePackages"
            }
        ]
    }
}
